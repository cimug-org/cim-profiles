   <xs:complexType name="Curve" sawsdl:modelReference="https://ucaiug.org/CIM101#Curve">
      <xs:annotation>
         <xs:documentation>A multi-purpose curve or functional relationship between an independent variable (X-axis) and dependent (Y-axis) variables.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="xMultiplier"
                     minOccurs="0"
                     maxOccurs="1"
                     type="m:UnitMultiplier"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#Curve.xMultiplier">
            <xs:annotation>
               <xs:documentation>Multiplier for X-axis.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="xUnit"
                     minOccurs="0"
                     maxOccurs="1"
                     type="m:UnitSymbol"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#Curve.xUnit">
            <xs:annotation>
               <xs:documentation>The X-axis units of measure.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CurveDatas"
                     minOccurs="1"
                     maxOccurs="unbounded"
                     type="m:CurveData"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#Curve.CurveDatas">
            <xs:annotation>
               <xs:documentation>The point data values that define this curve.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CurveData"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#CurveData">
      <xs:annotation>
         <xs:documentation>Multi-purpose data points for defining a curve. The use of this generic class is discouraged if a more specific class can be used to specify the X and Y axis values along with their specific data types.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="xvalue"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#CurveData.xvalue">
            <xs:annotation>
               <xs:documentation>The data value of the X-axis variable, depending on the X-axis units.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="y1value"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:float"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#CurveData.y1value">
            <xs:annotation>
               <xs:documentation>The data value of the  first Y-axis variable, depending on the Y-axis units.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="DistributedBid"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#DistributedBid">
      <xs:annotation>
         <xs:documentation>A distribution connected resource's offer to supply energy or associated energy services  or a bid to consume energy or associated energy services.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:ResourceBid">
            <xs:sequence>
               <xs:element name="scheduleType"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:ScheduleKind"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#DistributedBid.scheduleType">
                  <xs:annotation>
                     <xs:documentation>ScheduleType documents two key features for a specific resource operating mode: (a) the energy flow convention for a positive number as either production or consumption of service and (b) the sense of how the bid/offer amount is used as either a target or setpoint or as a relative change to a defined level.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="RegisteredDistributedResource"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:RegisteredDistributedResource"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#DistributedBid.RegisteredDistributedResource">
                  <xs:annotation/>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Document"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#Document">
      <xs:annotation>
         <xs:documentation>Parent class for different groupings of information collected and managed as a part of a business process. It will frequently contain references to other objects, such as assets, people and power system resources.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:IdentifiedObject">
            <xs:sequence>
               <xs:element name="comment"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:string"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#Document.comment">
                  <xs:annotation>
                     <xs:documentation>Free text comment.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="createdDateTime"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:dateTime"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#Document.createdDateTime">
                  <xs:annotation>
                     <xs:documentation>Date and time that this document was created.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="lastModifiedDateTime"
                           minOccurs="0"
                           maxOccurs="1"
                           type="xs:dateTime"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#Document.lastModifiedDateTime">
                  <xs:annotation>
                     <xs:documentation>Date and time this document was last modified. Documents may potentially be modified many times during their lifetime.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="IdentifiedObject"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#IdentifiedObject">
      <xs:annotation>
         <xs:documentation>This is a root class to provide common identification for all classes needing identification and naming attributes.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="mRID"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:string"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#IdentifiedObject.mRID">
            <xs:annotation>
               <xs:documentation>Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in IETF RFC 4122, for the mRID. The use of UUID is strongly recommended.</xs:documentation>
               <xs:documentation>For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="MarketProduct"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#MarketProduct">
      <xs:annotation>
         <xs:documentation>A product traded by an RTO (e.g. energy, 10 minute spinning reserve).  Ancillary service product examples include: Regulation, Regulation Up, Regulation Down, Spinning Reserve, Non-Spinning Reserve, etc.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="marketProductType"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:MarketProductType"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#MarketProduct.marketProductType">
            <xs:annotation>
               <xs:documentation>Market product type examples:</xs:documentation>
               <xs:documentation>EN (Energy)</xs:documentation>
               <xs:documentation>RU (Regulation Up)</xs:documentation>
               <xs:documentation>RD (Regulation Dn)</xs:documentation>
               <xs:documentation>SR (Spinning Reserve)</xs:documentation>
               <xs:documentation>NR (Non-Spinning Reserve)</xs:documentation>
               <xs:documentation>RC (RUC)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="MarketProductSelfSchedType"
                  sawsdl:modelReference="https://ucaiug.org/CIM101#MarketProductSelfSchedType">
      <xs:annotation>
         <xs:documentation>Market product self schedule bid types.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="PT">
            <xs:annotation>
               <xs:documentation>Price taker.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="MarketProductType"
                  sawsdl:modelReference="https://ucaiug.org/CIM101#MarketProductType">
      <xs:annotation>
         <xs:documentation>For example:</xs:documentation>
         <xs:documentation>Energy, Reg Up, Reg Down, Spin Reserve, Nonspin Reserve, RUC, Load Folloing Up, and Load Following Down.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="CO2e">
            <xs:annotation>
               <xs:documentation>A carbon dioxide equivalent is a metric measure used to compare the emissions from various greenhouse gases on the basis of their global-warming potential (GWP), by converting amounts of other gases to the equivalent amount of carbon dioxide with the same global warming potential.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EN">
            <xs:annotation>
               <xs:documentation>Energy generally priced per kWh or MWh.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LFD">
            <xs:annotation>
               <xs:documentation>Load following down (power withdrawal) generally priced per kW or MW.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LFU">
            <xs:annotation>
               <xs:documentation>Load following up (power injection) generally priced per kW or MW.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NR">
            <xs:annotation>
               <xs:documentation>Non-spinning Reserve or Replacement Reserve generally priced per kWh or MWh.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RC">
            <xs:annotation>
               <xs:documentation>Resources with available capacity for a forward market such as Day Ahead that are awarded due to their need for resource adequacy (i.e. reliability) purposes. RC represents the process for reliability unit commitment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RD">
            <xs:annotation>
               <xs:documentation>Frequency Regulation Service (energy withdrawal).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="REG">
            <xs:annotation>
               <xs:documentation>Frequency Regulation Service (injection and/or withdrawal).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RMD">
            <xs:annotation>
               <xs:documentation>Regulation Mileage Down.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RMU">
            <xs:annotation>
               <xs:documentation>Regulation Mileage Up.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RPD">
            <xs:annotation>
               <xs:documentation>RampDown: Power withdrawal generally priced per kW or MW.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RPU">
            <xs:annotation>
               <xs:documentation>RampUp: Power injection generally priced per kW or MW.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RU">
            <xs:annotation>
               <xs:documentation>Frequency Regulation Service (energy injection).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SR">
            <xs:annotation>
               <xs:documentation>Spinning Reserve or Contingency Reserve generally priced per kWh or MWh.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
