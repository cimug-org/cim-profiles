   <xs:element name="MarketDERBidOffer" type="m:MarketDERBidOffer"/>
   <xs:complexType name="MarketDERBidOffer">
      <xs:sequence>
         <xs:element name="DistributedBid"
                     type="m:DistributedBid"
                     minOccurs="1"
                     maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BasicIntervalSchedule"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#BasicIntervalSchedule">
      <xs:annotation>
         <xs:documentation>Schedule of values at points in time.</xs:documentation>
         <xs:documentation/>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="startTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#BasicIntervalSchedule.startTime">
            <xs:annotation>
               <xs:documentation>The time for the first time point. The value can be a time of day, not a specific date.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="value1Multiplier"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:UnitMultiplier"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#BasicIntervalSchedule.value1Multiplier">
            <xs:annotation>
               <xs:documentation>Multiplier for value1.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="value1Unit"
                     minOccurs="1"
                     maxOccurs="1"
                     type="m:UnitSymbol"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#BasicIntervalSchedule.value1Unit">
            <xs:annotation>
               <xs:documentation>Value1 units of measure.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Bid" sawsdl:modelReference="https://ucaiug.org/CIM101#Bid">
      <xs:annotation>
         <xs:documentation>Represents both bids to purchase and offers to sell energy or ancillary services in an RTO-sponsored market.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:Document">
            <xs:sequence>
               <xs:element name="ProductBids"
                           minOccurs="1"
                           maxOccurs="unbounded"
                           type="m:ProductBid"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#Bid.ProductBids">
                  <xs:annotation>
                     <xs:documentation>A bid comprises one or more product bids of market products</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="BidPriceCurve"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#BidPriceCurve">
      <xs:annotation>
         <xs:documentation>Specialized curve that represents an offer to supply services or a bid to consume services where the amount of service is defined on the X-axis and the asking price is defined on the Y1-axis. The amount of service use the xUnit and xMultiplier and the y1 price uses the y1Currency and optionally uses the y1unit and y1Multiplier if different than the xUnit and xMultiplier.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:Curve">
            <xs:sequence>
               <xs:element name="y1Currency"
                           minOccurs="0"
                           maxOccurs="1"
                           type="m:Currency"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#BidPriceCurve.y1Currency">
                  <xs:annotation>
                     <xs:documentation>Currency for the y1 axis in the BidPriceCurve.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="BidPriceSchedule"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#BidPriceSchedule">
      <xs:annotation>
         <xs:documentation>Defines bid schedules to allow a product bid to use specified bid price curves for different time intervals.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:BidPrice_RegularIntervalSchedule">
            <xs:sequence>
               <xs:element name="BidPriceCurve"
                           minOccurs="1"
                           maxOccurs="1"
                           type="m:BidPriceCurve"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#BidPriceSchedule.BidPriceCurve">
                  <xs:annotation/>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="BidPrice_BasicIntervalSchedule"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#BasicIntervalSchedule">
      <xs:annotation>
         <xs:documentation>Schedule of values at points in time.</xs:documentation>
         <xs:documentation/>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="startTime"
                     minOccurs="1"
                     maxOccurs="1"
                     type="xs:dateTime"
                     sawsdl:modelReference="https://ucaiug.org/CIM101#BasicIntervalSchedule.startTime">
            <xs:annotation>
               <xs:documentation>The time for the first time point. The value can be a time of day, not a specific date.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BidPrice_RegularIntervalSchedule"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#RegularIntervalSchedule">
      <xs:annotation>
         <xs:documentation>The schedule has time points where the time between them is constant.</xs:documentation>
         <xs:documentation/>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:BidPrice_BasicIntervalSchedule">
            <xs:sequence>
               <xs:element name="endTime"
                           minOccurs="1"
                           maxOccurs="1"
                           type="xs:dateTime"
                           sawsdl:modelReference="https://ucaiug.org/CIM101#RegularIntervalSchedule.endTime">
                  <xs:annotation>
                     <xs:documentation>The time for the last time point.  The value can be a time of day, not a specific date.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="BidSelfSched"
                   sawsdl:modelReference="https://ucaiug.org/CIM101#BidSelfSched">
      <xs:annotation>
         <xs:documentation>Defines self schedule values to be used for specified time intervals.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="m:RegularIntervalSchedule">
            <xs:sequence/>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
